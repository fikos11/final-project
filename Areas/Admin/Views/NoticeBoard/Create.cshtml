

@model Board

<form method="post" enctype="multipart/form-data">

    <div class="form-group">
        <label for="exampleInputEmail1">Vaxt</label>
        <input asp-for="@Model.Time">
        <span class="text-danger" asp-validation-for="@Model.Time)"></span>
    </div>
    <div class="form-group">
        <label>Basliq</label>
        <input asp-for="@Model.Title" class="form-control" aria-describedby="emailHelp">
